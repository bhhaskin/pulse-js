{
  "name": "@bhhaskin/pulse-js",
  "version": "0.1.2",
  "description": "Tiny analytics sdk for browsers. Send events with Beacon/fetch to your backend.",
  "author": "Bryan Haskin <bhhaskin@bitsofsimplicity.com>",
  "repository": "git@github.com:bhhaskin/pulse-js.git",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepublishOnly": "yarn lint && yarn build && yarn test"
  },
  "keywords": [
    "analytics",
    "tracking",
    "event",
    "typescript",
    "pulse"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitest/coverage-istanbul": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "eslint": "^9.24.0",
    "jsdom": "^26.1.0",
    "rollup": "^4.40.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "packageManager": "yarn@4.1.0",
  "dependencies": {
    "uuid": "^11.1.0"
  }
}
